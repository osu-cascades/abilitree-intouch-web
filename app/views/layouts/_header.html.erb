<!-- Files with a _ infront of it means it is a partial facility -->

<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "InTouch", root_path, id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Home",   root_path %></li>
        <li><%= link_to "Users",   users_show_path %></li>
          <li><%= link_to "Groups",   '/groups/show' %></li>
          <li><%= link_to "Notifications",   notifications_path %></li>
        <% if logged_in? %>
          <li><%= link_to "Log out", logout_path, method: :delete %> </li>
        <% else %>
          <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>